<div class="container">
  <div class="row">
    <div class="col-md-12">
      <h2>Your Profile</h2>
    </div>
  </div>
  <div class="row">
    <div class="col-md-6 col-sm-12">
      <% if @business["categories"] %>
        <%= image_tag seedImage(@business["categories"][0]["name"], "medium"), class: "img-thumbnail", size:'400' %>
      <% end %>
    </div>
    <div class="col-md-6 col-sm-12">
      <br/><br/>
      <div class="row">
        <div class="col-md-4">
          Business Name:
        </div>
        <div class="col-md-6">
          <%= @business['name'] %>
        </div>
      </div>
      <div class="row">
        <div class="col-md-4">
          Username:
        </div>
        <div class="col-md-6">
          <%= User.get_name(session['current_locu_id']) %>
        </div>
      </div>
      <div class="row">
        <div class="col-md-4">
          Locu Id:
        </div>
        <div class="col-md-6">
          <%= session['current_locu_id'] %>
        </div>
      </div>

